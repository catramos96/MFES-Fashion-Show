class Ticket
types
	public static TicketType = <Normal> | <VIP> | <Guest>;
instance variables
  public type : TicketType;
  public owner : Person;
  public show : FashionShow;

operations
	--Constructor
	public Ticket: TicketType * Person * FashionShow ==> Ticket
	Ticket(t,p,f) == (atomic(type := t; owner := p; show := f); return self);
end Ticket
