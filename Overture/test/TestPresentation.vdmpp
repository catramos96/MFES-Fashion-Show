class TestPresentation is subclass of TestClass
operations
	private testAddSpeaker: () ==> ()
 	testAddSpeaker() == (
	 dcl startTime: Utils`Date := mk_Utils`Date(1997,02,14,20,55);
 	 dcl endTime: Utils`Date := mk_Utils`Date(1997,02,14,21,55);
 	 dcl stage: FashionShow`Stage := mk_FashionShow`Stage(200, "B001", 1);
 	 dcl speaker1: Person := new Person("Person1", "111111111");
 	 dcl presentation: Presentation := new Presentation("Keynote", stage, startTime, endTime, speaker1);
 	 dcl speaker2: Person := new Person("Person2", "222222222");
 	 
   expectingNotIn(speaker2, presentation.workers);
   expectingNotIn(speaker2, presentation.speakers);
   
 	 presentation.addSpeaker(speaker2);
 	 
 	 expectingIn(speaker2, presentation.workers);
 	 expectingIn(speaker2, presentation.speakers);
 	);
 	
 	public static main: () ==> ()
	main() == (
	  dcl t: TestPresentation := new TestPresentation();
	  t.testAddSpeaker();
 	);
end TestPresentation
